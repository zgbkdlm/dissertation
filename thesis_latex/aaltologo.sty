  }{}%
  \ifthenelse{\equal{\AaltoLogoSchool}{ECON}}{%
    \setcounter{aaltologo_linecount}{1}%
    \iflanguage{finnish}{\ECONFI{#1}}{%
      \iflanguage{swedish}{\ECONSE{#1}}{%
        \ECONEN{#1}}}%
  }{}%
  \ifthenelse{\equal{\AaltoLogoSchool}{TKK}}{%
    \setcounter{aaltologo_linecount}{1}%
