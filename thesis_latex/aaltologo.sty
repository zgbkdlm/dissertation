    \definecolor{aaltoLightGreen}{cmyk}{.53,0,1,0}%
    \definecolor{aaltoOrange}{cmyk}{0,.64,1,0}%
    \definecolor{aaltoLightOrange}{cmyk}{0,.35,1,0}%
    \definecolor{aaltoFuchsia}{cmyk}{.27,.9,0,0}%
    % Old Turquoise, DEPRECATED
    \definecolor{aaltoTurquoise}{cmyk}{1,0,.3,0}%
  }{}%
  % Uncoated
  \ifthenelse{\equal{#1}{Uncoated}}{%
    % Basic colors
    \definecolor{aaltoBlack}{cmyk}{0,0,0,1}%
